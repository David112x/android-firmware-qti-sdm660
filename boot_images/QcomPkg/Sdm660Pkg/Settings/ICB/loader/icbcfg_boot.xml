   <global_def>
     <var_seq name="HWIOStructIncFile" type=DALPROP_DATA_TYPE_STRUCT_INC_FILE>
       ./../ICB/loader/icbcfg_data.c
     </var_seq> 
   </global_def>
   <device id="/dev/icbcfg/boot">
     <props name="icbcfg_info" type=DALPROP_ATTR_TYPE_STRUCT_PTR>
        icbcfg_info
     </props> 
   </device>   
</driver>
