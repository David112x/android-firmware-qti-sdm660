<image_cfg>
  <!-- Devcfg Environment Variables -->
  <devcfg_info>
    <target_img>hyp</target_img>
    <devcfg_target_build_tag>DAL_DEVCFG_HYP_IMG</devcfg_target_build_tag>
    <dal_config_target_id>0x8998</dal_config_target_id>
    <!-- These tags have to be comman separated. They can be on a single or multiple lines. -->
    <!-- Do not have the comments within devcfg_xml_tag tag -->
    <!--  TCSR_SOC_HW_VERSION : 0x7A8000, refer ipcat for HB family -->
        <SOC>
            <devcfg_xml_tag_single_configs>
                8998_xml
            </devcfg_xml_tag_single_configs>
        </SOC>
        <PLATFORM>
            <devcfg_platform_xml_tags>
            </devcfg_platform_xml_tags>
        </PLATFORM>
        <OEM>
            <uses_oem_config>true</uses_oem_config>
            <oem_config_base_addr>
            </oem_config_base_addr>
        </OEM>
        <soc_hw_version>
            8998_xml : 12290, 8998_xml : 12293
        </soc_hw_version>
    <tcsr>
        <tcsr_soc_hw_version_addr>
      		0x01FC8000
        </tcsr_soc_hw_version_addr>
        <tcsr_paddr_to_vaddr_same>true</tcsr_paddr_to_vaddr_same>
    </tcsr>
        <multiCfg>true</multiCfg>
    <memory>
        <props_memory_optimisation>true</props_memory_optimisation>
        <size_of_struct>true</size_of_struct>
        <hw_version_specific_props>true</hw_version_specific_props>
    </memory>
    
    <!-- The following tags are temporary and to be removed once not needed -->
    <DDIPlatform>false</DDIPlatform>
    
  </devcfg_info>
</image_cfg>
