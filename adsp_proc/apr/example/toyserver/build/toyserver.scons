TOYSERVER_SOURCES = [   
  '${BUILDPATH}/toyserver.c',
]

IMAGES = ['MULTIMEDIA_MODEM', 'MULTIMEDIA_QDSP6_SW', 'MULTIMEDIA_APPS']
env.AddLibrary(IMAGES, ('${BUILDPATH}/apr/toyserver'), TOYSERVER_SOURCES)
