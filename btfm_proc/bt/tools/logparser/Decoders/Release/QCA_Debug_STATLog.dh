GROUP gQca_STAT_ConnStats "Connection Stats"
{
    GROUP gQca_STAT_ActiveConnStats;
    /* Get Packet Stats Based on the ConnType */
    /* Read the Connection Type ahead to dynamically select the proper group for packet stats */
    FIELD fQca_ActiveConnType   START_BIT (Move 38 Bytes)   (Fixed 1 Byte) (Hex) SUPPRESS_DETAIL NO_MOVE
    BRANCH (FromTable   tQca_STATConnType   fQca_ActiveConnType)

    FIELD fQca_ActiveConnBBStat         (Fixed 4 Bytes)   (Hex)      "BBStats"

    FIELD fQca_ActiveConnBBStat_parse1  (Fixed 0)  RETRIEVE (StoreField fQca_ActiveConnBBStat) ALSO (ShiftRight 0)  ALSO (AndMask 0x000f)  (Table tQca_BBStats)   "BBStatus_1"
    FIELD fQca_ActiveConnBBStat_parse2  (Fixed 0)  RETRIEVE (StoreField fQca_ActiveConnBBStat) ALSO (ShiftRight 4)  ALSO (AndMask 0x000f)  (Table tQca_BBStats)   "BBStatus_2"
    FIELD fQca_ActiveConnBBStat_parse3  (Fixed 0)  RETRIEVE (StoreField fQca_ActiveConnBBStat) ALSO (ShiftRight 8)  ALSO (AndMask 0x000f)  (Table tQca_BBStats)   "BBStatus_3"
    FIELD fQca_ActiveConnBBStat_parse4  (Fixed 0)  RETRIEVE (StoreField fQca_ActiveConnBBStat) ALSO (ShiftRight 12) ALSO (AndMask 0x000f)  (Table tQca_BBStats)   "BBStatus_4"
    FIELD fQca_ActiveConnBBStat_parse5  (Fixed 0)  RETRIEVE (StoreField fQca_ActiveConnBBStat) ALSO (ShiftRight 16) ALSO (AndMask 0x000f)  (Table tQca_BBStats)   "BBStatus_5"
    FIELD fQca_ActiveConnBBStat_parse6  (Fixed 0)  RETRIEVE (StoreField fQca_ActiveConnBBStat) ALSO (ShiftRight 20) ALSO (AndMask 0x000f)  (Table tQca_BBStats)   "BBStatus_6"
    FIELD fQca_ActiveConnBBStat_parse7  (Fixed 0)  RETRIEVE (StoreField fQca_ActiveConnBBStat) ALSO (ShiftRight 24) ALSO (AndMask 0x000f)  (Table tQca_BBStats)   "BBStatus_7"
    FIELD fQca_ActiveConnBBStat_parse8  (Fixed 0)  RETRIEVE (StoreField fQca_ActiveConnBBStat) ALSO (ShiftRight 28) ALSO (AndMask 0x000f)  (Table tQca_BBStats)   "BBStatus_8"

}

